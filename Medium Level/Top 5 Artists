with cte1 as(
SELECT 
t1.artist_name,
t2.name as song_name
from artists as t1
left join songs as t2
using(artist_id)
left join global_song_rank as t3
using(song_id)
where t3.rank <= 10)

select *
FROM
(select 
  artist_name,
dense_rank() over(order by cnt desc) as artist_rank
from
(SELECT
  artist_name,
  count(*) as cnt
from cte1
group by artist_name) as t10) as t11
where artist_rank <= 5;
